{
  "name": "formify-vue",
  "version": "1.9.0",
  "private": false,
  "type": "module",
  "files": [
    "dist",
    "LICENSE"
  ],
  "main": "dist/formify-vue.umd.cjs",
  "module": "dist/formify-vue.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/formify-vue.js",
      "require": "./dist/formify-vue.umd.cjs"
    },
    "./style.css": "./dist/formify-vue.css"
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\"",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.15",
    "tailwindcss": "^4.0.15",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.11",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "semantic-release": "^24.2.3",
    "typescript": "~5.8.0",
    "vite": "^6.3.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.2.8"
  },
  "license": "MIT"
}
